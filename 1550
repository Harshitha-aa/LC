bool threeConsecutiveOdds(int* arr, int arrSize) {
    int c=0;
    if(arrSize>=3)
    {
    for(int i=arrSize-1;i>=0;i--)
    {
       if(arr[i]%2!=0)
       {
        c++;
        if(c==3)
        {
            return true;
        }
       }
       else
       {
        c=0;
       }
    }
    return false;
    }
    return false;
}
